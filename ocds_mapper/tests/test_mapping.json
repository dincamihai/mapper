{
    "publisher": {
        "name": "{name}"
    },
    "publishingMeta": {
        "date": "{date}"
    },
    "releases": [
        {.repeated section csv_rows}
            {.meta-left}
                "releaseMeta": {
                    "ocid": "{reference_number}",
                    "locale": "en_us",
                    "releaseID": "{releaseID}",
                    "formation": {
                        "attachments": [
                            {.repeated section document|list}
                                {.meta-left}
                                    "uri": "{@}",
                                    "name": ""
                                {.meta-right}{.alternates with},
                            {.end}
                        ]
                    }
                }
            {.meta-right}{.alternates with},
        {.end}
        {.newline}
    ]
}
